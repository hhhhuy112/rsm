<div class="container">
  <div class="profile clearfix">
    <%= render @user %>
  </div>
  <div class="tab-custom row">
    <div class="col-lg-2 col-sm-3 col-xs-3 tab-custom-menu">
      <div class="list-group">
        <a href="javascript:void(0)" class="list-group-item active text-center">
          <i class="fa fa-user"></i><br/><%= t ".profile" %>
        </a>
        <a href="javascript:void(0)" class="list-group-item text-center">
          <i class="fa fa-cc-diners-club"></i><br/><%= t ".clubs" %>
