<div class="container">

  <div class="row">
    <div class="col-md-3 col-sm-4 col-xs-4 items-profile bg-white">
      <%= render @user %>
      <ul class="nav nav-pills nav-stacked tab-title ">
        <li class="active">
          <a href="#tab_a" class="item-profile" data-toggle="pill">
            <i class="fa fa-user"></i><%= t ".profile" %>
          </a>
        </li>

        <li>
          <a href="#tab_b" class="item-profile" data-toggle="pill">
          <i class="fa fa-cc-diners-club"></i><%= t ".clubs" %>
          </a>
        </li>

        <li>
          <a href="#tab_c" class="item-profile" data-toggle="pill">
            <i class="fa fa-certificate"></i><%= t ".certificates" %>
          </a>
        </li>


        <li>
          <a href="#tab_d" class="item-profile" data-toggle="pill">
            <i class="fa fa-bank"></i><%= t ".experiences" %>
          </a>
        </li>

        <li>
          <a href="#tab_e" class="item-profile" data-toggle="pill">
            <i class="fa fa-trophy"></i><%= t ".achievements" %>
          </a>
        </li>
      </ul>
    </div>


    <div class="col-md-9 col-sm-8 col-xs-8" id="contents">
      <div class="tab-content col-md-12 bg-white">

        <div class="tab-pane active" id="tab_a">
          <%= render "users/index" %>
        </div>


        <div class="tab-pane" id="tab_b">
          <%= render "clubs/index" %>
        </div>

        <div class="tab-pane" id="tab_c">
          <%= render "certificates/index" %>
        </div>

        <div class="tab-pane" id="tab_d">
          <%= render "experiences/index" %>
        </div>

        <div class="tab-pane" id="tab_e">
          <%= render "achievements/index" %>
        </div>
      </div>
    </div>
  </div>
</div>
<a href="#" id="to-top" class="to-top"><i class="fa fa-angle-up"></i></a>
