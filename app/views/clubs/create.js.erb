<% if @club.errors.any? %>
  $('#btn-add-club').hide();
  $('#area_form_club').html('<%= j render "clubs/form" %>');
<% else %>
  $('.placeholder-club').hide();
  $('#area_form_club').remove();
  $('#btn-add-club').show();
  $('#form-club').append('<%= j render @club %>');
  alertify.success('<%= @message %>');
<% end %>
