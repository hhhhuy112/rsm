<% if @certificate.errors.any? %>
  $('#area_form_certificate_<%= @certificate.id %>').html('<%= j render "certificates/form" %>');
<% else %>
  $('#edit_certificate_<%= @certificate.id %>').remove();
  $('#info_certificate_<%= @certificate.id %>').replaceWith('<%= j render @certificate %>').show();
  alertify.success('<%= @message %>');
<% end %>
