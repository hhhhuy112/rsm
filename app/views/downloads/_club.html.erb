<div class="bound" id="level">
  <div class="sub_item mgb10">
    <table class="w100" cellpadding="0" cellspacing="0" border="0">
      <tbody>
        <tr>
          <td class="w75">
            <p data-tooltip-position="bottom">
              <div class="w100 b tup ip f12 cv7_color"><%= club.name %></div>
            </p>
            <p data-tooltip-position="bottom">
              <div class="w50 tcap ip f13 tg"><%= club.position %></div>
            </p>
            <p data-tooltip-position="bottom">
              <div class="w100 f13 tg"><%= club.content %></div>
            </p>
          </td>
          <td class="w25 text_r">
            <span data-tooltip-position="bottom">
              <div class="f12 ip w40 tg abloi">
                <%= l club.start_time, format: :long %>-
                <% if club.end_time.present? %>
                  <%= l club.end_time, format: :long %>
                <% end %>
              </div>
            </span>
          </td>
        </tr>
     </tbody>
    </table>
  </div>
</div>
