<% if @appointment.errors.any? %>
  $("#appointment_apply_<%= @apply.id %>").html('<%= j (render partial: "employers/appointments/form", locals: {apply: @apply, members: @members, template_members: @template_members, template_users:  @template_users}) %>');
<% else %>
  $('#new_appointment')[0].reset();
  $('#myModal_<%= @apply.id %>').modal('hide');
  alertify.success('<%= @message %>');
<% end %>
$('#img-loading').hide();
$('.config').css('opacity', 1);
