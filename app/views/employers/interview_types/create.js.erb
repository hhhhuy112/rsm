<% if @error %>
  alertify.error('<%= @error %>')
  <% if @interview_type.errors.any? %>
    alertify.error('<%= @interview_type.errors.full_messages %>');
  <% end %>
<% elsif @success %>
  alertify.success('<%= @success %>');
  $('#<%= params[:div_id]%>').html('<%= j render partial: "employers/interview_types/interview_type", locals: {interview_type: @interview_type} %>')
<% end %>
