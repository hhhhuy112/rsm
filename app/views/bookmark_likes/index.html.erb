<div class="container">
  <input id="tab1" type="radio" name="tabs" checked>
  <label for="tab1"><%= t "apply.bookmark" %></label>
  <section id="content1" class="show_bookmark">
    <% if @bookmark_likes.blank? %>
      <div class="placeholder-section box-md">
        <p class="placeholder" style="text-align: center;">
          <i class="fa fa-heart" aria-hidden="true"></i>
          <%= t "apply.havenot" %>
        </p>
      </div>
    <% end %>
    <div id ="bookmark">
      <%= render @bookmark_likes %>
    </div>
    <div id = "pag_bookmark" class = "row text-center">
      <%= paginate @bookmark_likes %>
    </div>
  </section>
</div>
